{
  "name": "vue-ssr",
  "version": "1.0.0",
  "description": "a vue ssr example project",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server config client/mixins",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:test": "pm2 start pm2-config.json --env test",
    "start:prod": "pm2 start pm2-config.json --env prod",
    "build:test": "npm run build:client-test && npm run build:server-test",
    "build:client-test": "cross-env NODE_ENV=test webpack --config build/webpack.client.config.js",
    "build:server-test": "cross-env NODE_ENV=test  webpack --config build/webpack.server.config.js",
    "build:prod": "npm run build:client-prod && npm run build:server-prod",
    "build:client-prod": "cross-env NODE_ENV=production  webpack --config build/webpack.client.config.js",
    "build:server-prod": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js",
    "lint": "eslint --init"
  },
  "author": "chenjun",
  "license": "ISC",
  "dependencies": {
    "@sentry/browser": "^5.1.0",
    "chokidar": "^2.0.4",
    "consola": "^1.4.4",
    "ejs": "^2.6.1",
    "element-ui": "^2.7.2",
    "error-formatter": "^1.0.5",
    "express": "^4.16.4",
    "idempotent-babel-polyfill": "^7.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "pm2": "^3.2.9",
    "popper.js": "^1.14.6",
    "request": "^2.88.0",
    "vue": "^2.5.17"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "eslintIntegration": false
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@nuxtjs/friendly-errors-webpack-plugin": "^2.0.2",
    "@vue/babel-plugin-transform-vue-jsx": "^1.0.0-beta.1",
